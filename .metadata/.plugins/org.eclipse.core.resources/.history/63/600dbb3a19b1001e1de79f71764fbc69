package Chapter8_Example;

public class Car extends Vehicle{
	private int gas;
	private double price;
	
	
	


	@Override
	public double calcTax() {
		if(gas >= 3000) {
			tax = price*0.05;
		}else if(gas < 3000 && gas >= 1500) {
			tax = price*0.03;
		}else {
			tax = price*0.01;
		}
		return tax;
	}
	

}
