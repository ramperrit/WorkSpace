package Chapter6_Example;

import java.util.*;

class Student {
	String name;
	int ban = 0;
	int no = 0;
	int kor = 0;
	int eng = 0;
	int math = 0;
	
	int sum;
	float avg;
	
	int getTotal() {
		sum = kor + eng + math;
		return sum;
	}
	
	float getAverage() {
		avg = Math.round((sum/3)*100.0)/100;
		return avg;
	}
}
