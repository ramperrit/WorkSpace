package Chapter06;

public class Calculator {

	boolean power = false;
	
	
	//전원 켜기
	void powerOn() {
		this.power = true;
		System.out.println("전원을 켭니다.");
	}
	//전원 끄기
	void powerOff() {
		this.power = false;
		System.out.println("전원을 끕니다.");
	}
	
	int plus(int x, int y) {
		return x + y;
	}
	int minus(int x, int y) {
		return x - y;
	}
}
