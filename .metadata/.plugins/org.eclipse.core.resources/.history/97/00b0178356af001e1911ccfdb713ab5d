package Chapter6;

public class CarExam2 {

	public static void main(String[] args) {
		Car car1 = new Car(); //자동차 인스턴스 1
		Car car2 = new Car(); //자동차 인스턴스 2
		
		car1.gas = 10;
		car2.gas = 20;
		
		System.out.println("car1's gas : "+car1.gas);
		System.out.println("car2's gas : "+car2.gas);
	}

}
