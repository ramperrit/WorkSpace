package Chapter8_Example;

public abstract class Vehicle {
	
	private String model;
	private int price;
	double tax;
	int gas;
	int weight;
	
	
	public abstract double calcTax();
	
	
	public abstract String getInfo();


	public String getModel() {
		return model;
	}


	public void setModel(String model) {
		this.model = model;
	}
}
