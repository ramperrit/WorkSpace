package Chapter6_Example;

class Student {
	String name;
	int ban = 0;
	int no = 0;
	int kor = 0;
	int eng = 0;
	int math = 0;
	
	int sum = 0;
	float avg = 0.0f;
	
	int getTotal() {
		sum = kor + eng + math;
		return sum;
	}
	
	float getAverage() {
		avg = (float)Math.round((sum/3f)*100f)/100f;
		return avg;
	}
}
