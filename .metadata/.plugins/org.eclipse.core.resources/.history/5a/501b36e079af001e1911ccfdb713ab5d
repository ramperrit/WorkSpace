package Chapter6_Example;

public class SutdaCard {

	SutdaCard(boolean isKwang){
		System.out.println("광이면 true, 아니면 false");
	}
	
	int num = 0;
	boolean isKwang = true;
	
	SutdaCard(int num, boolean isKwang){
		if(num < 1 || num > 11) {
			System.out.println("카드의 숫자.(1~10사이의 정수)");
		}else {
			this.num = num;
		}
		if(isKwang = true) {
			System.out.println(num+"K");
		}
		
	}
}
