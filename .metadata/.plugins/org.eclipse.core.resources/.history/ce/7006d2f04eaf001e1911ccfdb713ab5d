package Chapter6;

public class Rectangle {
	boolean create = false;
	
	double area(double x, double y) {
		double result = 0;
		result = x * y;
		System.out.println("직사각형의 넓이 : " + result);
		return result;
	}
	
	double round(double x, double y) {
		double result = 0;
		result = 2*(x+y);
		System.out.println("직사각형의 둘래 : " + result);
		return result;
	}
	
	void createOn() {
		this.create();
	}

}
