package Chapter8_Example;

public abstract class Vehicle {
	
	private String model;
	private int price;
	double tax;
	int gas;
	int weight;
	
	
	public abstract double calcTax();
	public void Car(String model, int price, int gas) {
		this.model = model;
		this.price = price;
		this.gas = gas;
	}
	
	public abstract String getInfo();
}
