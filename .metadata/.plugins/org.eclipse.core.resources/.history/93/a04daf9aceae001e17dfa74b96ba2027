package Chapter06;

public class CalculatorExam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Calculator cal = new Calculator();
		cal.plus(10,10);
		cal.powerOn();
		System.out.println(cal.plus(10,20));
		System.out.println(cal.minus(10,20));
		cal.powerOff();
	}

}
