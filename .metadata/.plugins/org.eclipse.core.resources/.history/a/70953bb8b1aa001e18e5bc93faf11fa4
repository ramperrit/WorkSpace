
public class Hello {
	public static void main(String[] args) {
		String str1 = "10";
		byte a = Byte.parseByte(str1);
		System.out.println(a);
		
		String str2 = "200";
		short b = Short.parseShort(str2);
		System.out.println(b);
		
		String str3 = "123.45";
		double c = Double.parseDouble(str3);
		System.out.println(c);
		
		String str4 = "true";
		boolean d = Boolean.parseBoolean(str4);
		System.out.println(d);
		
		float e = 135.456f;
		String str5 = String.valueOf(e);
		System.out.println(str5 + 2);
	}
}
