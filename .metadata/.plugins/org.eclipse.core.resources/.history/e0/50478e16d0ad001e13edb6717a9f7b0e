package Chapter5;

public class ArryExam {

	public static void main(String[] args) {
		int[] heights = new int[5];
		heights [0] = 170;
		heights [1] = 174;
		heights [2] = 176;
		heights [3] = 179;
		heights [4] = 180;
		for (int i=0; i<heights.length;i++) {
			System.out.println((i+1)+"번째 학생의 키 : "+heights[i]);
		}
	}

}
