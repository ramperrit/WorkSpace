package Chapter7;

public class AnimalExam {

	public static void main(String[] args) {
//		Animal animal = new Animal(); // 구현 불가능 : 추상클래스
		
		Cat cat = new Cat();
		animalSound(cat);
		
		Dog dog = new Dog();
		animalSound(dog);
	}
	public static void animalSound(Animal animal) {
		animal.sound();
	}

}
